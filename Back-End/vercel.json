{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/products",
        "dest": "index.js"
      },
      {
        "src": "/products/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/categories",
        "dest": "index.js"
      },
      {
        "src": "/categories/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/brands",
        "dest": "index.js"
      },
      {
        "src": "/brands/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/users",
        "dest": "index.js"
      },
      {
        "src": "/users/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/auth",
        "dest": "index.js"
      },
      {
        "src": "/auth/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/auth/reset-password?(.*)",
        "dest": "index.js"
      },
      {
        "src": "/cart",
        "dest": "index.js"
      },
      {
        "src": "/cart/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/orders",
        "dest": "index.js"
      },
      {
        "src": "/orders/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/wishlist",
        "dest": "index.js"
      },
      {
        "src": "/wishlist/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/payment",
        "dest": "index.js"
      },
      {
        "src": "/payment/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/",
        "dest": "build/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "build/$1"
      }
    ]
  }
